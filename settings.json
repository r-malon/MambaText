{
	"title": "MambaText",
	"lang_file": "languages/portuguese.json",
	"scheme": "schemes/monokai.json",
	"size": "720x480",
	"minsize": [250, 450],
	"icon": "icon.ico",
	"insert_width": 1,
	"insertofftime": 200,
	"insertontime": 500,
	"min_font_size": 6,
	"max_font_size": 26,
	"font_size_interval": 2,
	"filetypes": [
		["All Files", "*.*"], 
		["Text Files", "*.txt"], 
		["SQL Files", "*.sql"], 
		["Python Scripts", "*.py"], 
		["C/C++ Files", "*.cpp *.c *.h"], 
		["Java Files", "*.java"], 
		["Markdown Documents", "*.md"], 
		["HTML Documents", "*.html"], 
		["CSS Documents", "*.css"], 
		["JavaScript Files", "*.js"], 
		["JSON Documents", "*.json"]
	],
	"fonts": [
		"Arial",
		"Times",
		"Courier",
		"Georgia",
		"Verdana",
		"Comic Sans MS",
		"Trebuchet MS"
	],
	"shortcuts": {
		"menu_hider": "<Alt-m>",
		"maximize": "<F11>",
		"new_file": "<Control-n>",
		"save": "<Control-s>",
		"saveas": "<Control-Shift-S>",
		"open": "<Control-o>",
		"find": "<Control-f>",
		"replace": "<Control-h>",
		"clear_highlights": "<Control-l>",
		"redo": "<Control-Shift-Z>",
		"un_pause": "<Control-p>"
	}
}